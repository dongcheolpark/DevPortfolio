/// <backend>
		{
			"name": "npm backend test",
			"request": "launch",
			"runtimeArgs": [
				"test",
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
			"cwd": "${workspaceRoot}/backend"
		},
		{
			"name": "npm backend start",
			"request": "launch",
			"runtimeArgs": [
				"start",
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
			"cwd": "${workspaceRoot}/backend"
		},
/// </backend>
/// <frontend>
		{
			"name": "npm frontend serve",
			"request": "launch",
			"runtimeArgs": [
				"run",
				"serve",
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
			"cwd": "${workspaceRoot}/frontend/devportfolio"
		},
		{
			"name": "npm frontend build",
			"request": "launch",
			"runtimeArgs": [
				"run",
				"serve",
			],
			"runtimeExecutable": "npm",
